<script type="text/javascript">
     counter = function() {
        var value = $('#TXT_Abstract').val();
        if (value.length == 0) {
            $('#wordCount').html(300);
            return;
        }
        var regex = /\s+/gi;
        var wordCount = value.trim().replace(regex, ' ').split(' ').length;
        $('#wordCount').html(300-wordCount);
    };
    $(document).ready(function() {
        $('#TXT_Abstract').change(counter);
        $('#TXT_Abstract').keydown(counter);
        $('#TXT_Abstract').keypress(counter);
        $('#TXT_Abstract').keyup(counter);
        $('#TXT_Abstract').blur(counter);
        $('#TXT_Abstract').focus(counter);
        $('#otherPresentingAuthor').css("display", "none");
        $('#otherCorrespondingAuthor').css("display", "none");
    });
    $('#DDList_PresentingAuthorInstitution').change(function() {
        if (this.value == 'Other') {
            $('#otherPresentingAuthor').css("display", "block");
        }
        else {
            $('#otherPresentingAuthor').css("display", "none");
        }
    });
    $('#DDList_CorrespondingAuthorInstitution').change(function() {
        if (this.value == 'Other') {
            $('#otherCorrespondingAuthor').css("display", "block");
        }
        else {
            $('#otherCorrespondingAuthor').css("display", "none");
        }
    });
</script>
