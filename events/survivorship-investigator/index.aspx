<script type="text/javascript">
     counter = function() {
        var value = $('#TXT_Bio').val();
        if (value.length == 0) {
            $('#wordCount').html(250);
            return;
        }
        var regex = /\s+/gi;
        var wordCount = value.trim().replace(regex, ' ').split(' ').length;
        $('#wordCount').html(250-wordCount);
    };
    $(document).ready(function() {
        $('#TXT_Bio').change(counter);
        $('#TXT_Bio').keydown(counter);
        $('#TXT_Bio').keypress(counter);
        $('#TXT_Bio').keyup(counter);
        $('#TXT_Bio').blur(counter);
        $('#TXT_Bio').focus(counter);
        $('#LunchDay1Selection').css("display", "none");
        $('#LunchDay2Selection').css("display", "none");
    });

    $('input[type=radio][name=RDList_BoxLunchDay1]').change(function() {
        if (this.value == 'TRUE') {
            $('#LunchDay1Selection').css("display", "block");
        }
        else if (this.value == 'FALSE') {
            $('#LunchDay1Selection').css("display", "none");
        }
    });

    $('input[type=radio][name=RDList_BoxLunchDay2]').change(function() {
        if (this.value == 'TRUE') {
            $('#LunchDay2Selection').css("display", "block");
        }
        else if (this.value == 'FALSE') {
            $('#LunchDay2Selection').css("display", "none");
        }
    });
</script>
