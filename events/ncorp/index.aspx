<script type="text/javascript">
    ctrltxt = document.getElementById("Lbl_Institution_Other");
    ctrltxt.style.display = 'none';
    ctrltxt = document.getElementById("Lbl_Role_Other");
    ctrltxt.style.display = 'none';
    ctrltxt = document.getElementById("Lbl_Title_Other");
    ctrltxt.style.display = 'none';
    $('#DDList_Program option').mousedown(function(e) {
        e.preventDefault();
        jQuery(this).toggleClass('selected');
        jQuery(this).prop('selected', !jQuery(this).prop('selected'));
        if($('#DDList_Program').val() == "Other") {
            $("#Lbl_Institution_Other").show();
        } else {
            $("#Lbl_Institution_Other").hide();
        } 
        return false;
    });
    $('#DDList_Role option').mousedown(function(e) {
        e.preventDefault();
        jQuery(this).toggleClass('selected');
        jQuery(this).prop('selected', !jQuery(this).prop('selected'));
        if($('#DDList_Role').val() == "Other") {
            $("#Lbl_Role_Other").show();
        } else {
            $("#Lbl_Role_Other").hide();
        } 
        return false;
    });
    $('#TXT_Title option').mousedown(function(e) {
        e.preventDefault();
        jQuery(this).toggleClass('selected');
        jQuery(this).prop('selected', !jQuery(this).prop('selected'));
        console.log($('#TXT_Title').val());
        if(jQuery.inArray("Other", $('#TXT_Title').val()) != -1) {
            $("#Lbl_Title_Other").show();
        } else {
            $("#Lbl_Title_Other").hide();
        } 
        return false;
    });
    
    $('#RDList_MeetingParticipation input').mousedown(function(e) {
        e.preventDefault();
        ValidateCheckBoxList();
    });
    
    function ValidateCheckBoxList(sender, args) {
        var checkBoxList = document.getElementById("<%=RDList_MeetingParticipation.ClientID %>");
        var checkboxes = checkBoxList.getElementsByTagName("input");
        var isValid = false;
        for (var i = 0; i < checkboxes.length; i++) {
            if (checkboxes[i].checked) {
                isValid = true;
                break;
            }
        }
        args.IsValid = isValid;
    }
</script>


